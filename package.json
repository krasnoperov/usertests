{
  "name": "usertests",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm:dev:worker\" \"npm:dev:frontend\"",
    "dev:frontend": "vite",
    "dev:worker": "wrangler dev --config wrangler.dev.toml --port 8788 --persist-to .wrangler/state",
    "build": "vite build",
    "deploy": "npm run build && wrangler deploy",
    "deploy:production": "npm run build && wrangler deploy --env production",
    "deploy:stage": "npm run build && wrangler deploy",
    "test": "node --env-file .env --import @swc-node/register/esm-register --test",
    "lint": "eslint \"{src,scripts}/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "db:migrate": "node --import @swc-node/register/esm-register scripts/db-migrate.ts",
    "db:migrate:stage": "node --import @swc-node/register/esm-register scripts/db-migrate.ts stage remote",
    "db:migrate:production": "node --import @swc-node/register/esm-register scripts/db-migrate.ts production remote",
    "cli": "node --env-file .env --import @swc-node/register/esm-register src/cli/index.ts"
  },
  "dependencies": {
    "@google/genai": "^1.30.0",
    "@openai/agents": "^0.3.3",
    "@react-oauth/google": "^0.12.2",
    "@tanstack/react-query": "^5.90.11",
    "better-sqlite3": "^12.4.6",
    "dotenv": "^17.2.3",
    "hono": "^4.10.6",
    "inversify": "^7.10.4",
    "jose": "^6.1.2",
    "kysely": "^0.28.8",
    "kysely-d1": "^0.4.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
    "reflect-metadata": "^0.2.2",
    "remark-gfm": "^4.0.1",
    "uuid": "^13.0.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@cloudflare/ai": "^1.2.2",
    "@cloudflare/vite-plugin": "^1.15.2",
    "@cloudflare/workers-types": "^4.20251125.0",
    "@csstools/postcss-light-dark-function": "^2.0.11",
    "@swc-node/register": "^1.11.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "express": "^5.1.0",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "postcss-preset-env": "^10.4.0",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.2.4",
    "vite-plugin-mkcert": "^1.17.9",
    "wrangler": "^4.63.0"
  }
}
